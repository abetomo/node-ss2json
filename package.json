{
  "name": "ss2json",
  "version": "3.3.0",
  "description": "Convert Google Spreadsheet contents to JSON. Also update Google spreadsheet from JSON data.",
  "keywords": [
    "json",
    "google",
    "spreadsheet",
    "convert",
    "update"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "lint:md": "remark .",
    "test": "npm run lint && npm run lint:md && npm run unit",
    "unit": "jest"
  },
  "bin": {
    "ss2json": "bin/ss2json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abetomo/node-ss2json.git"
  },
  "author": "abetomo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abetomo/node-ss2json/issues"
  },
  "homepage": "https://github.com/abetomo/node-ss2json#readme",
  "devDependencies": {
    "jest": "^29.3.0",
    "remark-cli": "^11.0.0",
    "remark-preset-lint-recommended": "^6.1.1",
    "request": "^2.88.2",
    "standard": "^17.0.0"
  },
  "dependencies": {
    "commander": "^9.4.1",
    "googleapis": "^109.0.1"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "__mocks__",
    "__tests__",
    "bin",
    "index.js",
    "lib",
    "package.json"
  ],
  "jest": {
    "collectCoverage": true
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      [
        "remark-lint-no-shortcut-reference-link",
        false
      ],
      [
        "remark-lint-no-undefined-references",
        false
      ],
      [
        "remark-lint-list-item-indent",
        "space"
      ]
    ]
  },
  "engines": {
    "node": ">= 8.0.0"
  }
}
